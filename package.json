{
  "name": "cheerio-httpcli",
  "version": "0.3.3",
  "description": "http client module with cheerio & iconv(-lite) & promise",
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "cheerio": "^0.19.0",
    "ent": "^2.2.0",
    "iconv-lite": "^0.4.13",
    "jschardet": "^1.3.0",
    "prettyjson": "^1.1.3",
    "request": "^2.65.0",
    "rsvp": "^3.1.0"
  },
  "devDependencies": {
    "blanket": "*",
    "espower-loader": "*",
    "intelli-espower-loader": "*",
    "mocha": "*",
    "mocha-clean": "*",
    "node-static": "*",
    "power-assert": "*",
    "type-of": "*"
  },
  "directories": {
    "test": "test/"
  },
  "config": {
    "blanket": {
      "pattern": [
        "lib"
      ],
      "data-cover-never": [
        "node_modules",
        "test",
        "vendor"
      ]
    }
  },
  "scripts": {
    "test": "mocha --opts test/mocha-cov.opts > coverage.html && true || true",
    "lint": "eslint test/*.js lib/*.js example/*.js && true || true",
    "cs": "jscs test lib example && true || true"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ktty1220/cheerio-httpcli.git"
  },
  "keywords": [
    "cheerio",
    "http",
    "dom",
    "scrape"
  ],
  "author": {
    "name": "ktty1220",
    "email": "ktty1220@gmail.com"
  },
  "license": "MIT",
  "readmeFilename": "README.md"
}
