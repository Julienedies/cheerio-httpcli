    "object-assign": "^4.0.1",
    "os-locale": "^1.4.0",
    "prettyjson": "^1.1.3",
    "request": "^2.67.0",
    "require-uncached": "^1.0.2",
    "rsvp": "^3.1.0",
    "spawn-sync": "^1.0.14",
